{
    "version": 2,
    "builds": [
      {
        "src": "./server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/toggl",
        "dest": "server.js"
      },
      {
        "src": "/api/todoist",
        "dest": "./server.js"
      },
      {
      "src": "/",
      "dest": "/"
      }
    ]
  }